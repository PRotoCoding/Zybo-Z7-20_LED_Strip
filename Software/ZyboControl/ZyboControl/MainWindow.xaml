        xmlns:local="clr-namespace:ZyboControl"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="185,10,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="serialPortLabel" Content="Serial Port" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="sendButton" Content="Send" HorizontalAlignment="Left" Margin="206,56,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>

    </Grid>
